        "express-validator": "^6.14.1",
        "html-pdf": "^3.0.1",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@types/chai": "^4.3.1",
        "@types/cors": "^2.8.12",
        "@types/ejs": "^3.1.1",
        "@types/mocha": "^9.1.1",
        "@typescript-eslint/eslint-plugin": "^5.28.0",
        "@typescript-eslint/parser": "^5.28.0",
